---
import PageFooter from "@/components/page-footer.astro";
import Layout from "@/layouts/layout.astro";
import { EMAIL, EMAIL_LINK } from "@/lib/constants";
---

<Layout title="Contact">
  <section>
    <button
      onclick="history.back()"
      class="w-full h-full text-sm text-muted-foreground hover:bg-muted-foreground hover:text-muted transition-colors px-4 py-0.5"
    >
      back
    </button>
  </section>
  <section class="p-4 border-t">
    <h1>Contact</h1>
    <p>email: <a href={EMAIL_LINK} target="_blank" class="link">{EMAIL}</a></p>
  </section>
  <PageFooter lastUpdated={null} />
</Layout>
